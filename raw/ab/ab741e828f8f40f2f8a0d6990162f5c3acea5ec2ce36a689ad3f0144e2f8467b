/* ===========================================
color of EXAMPLE
0: default
1: blue
============================================= */
ini_colorExample = 0


/* ===========================================
Hovering over menu items or title bar for a specified amount of time will trigger CLICK EVENT.
This option only applies to desktop.

	ini_menuHover: 1=enabled
	ini_menuHover_delay: The time offset, from the moment at which 
		the 'CLICK EVENT' is triggered. This should be specified in milliseconds.
		1000 milliseconds = 1 seconds 
============================================= */
ini_menuHover = 0
ini_menuHover_delay = 1500


/* ===========================================
Show/Hide etymology
0: show(default)
1: hide
============================================= */
ini_etymology_hide = 0


/* ===========================================
Show/Hide Word Family
0: show(default)
1: hide
============================================= */
ini_wordfamily_hide = 0

