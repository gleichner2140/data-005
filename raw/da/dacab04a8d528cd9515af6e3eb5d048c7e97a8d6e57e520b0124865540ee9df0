    "      )\n",
    "\n",
    "# 内圈团队数量\n",
    "ax.pie(vals.flatten(), radius=1-size,colors=inner_colors,labels=['']*12+[x for x in range(1,13)],\n",
    "       labeldistance=.8,wedgeprops=dict(width=size, edgecolor='w')\n",
    "      )\n",
    "\n",
    "ax.set(aspect=\"equal\")\n",
    "\n",
